{
  "name": "enupal/translate",
  "description": "Translation management plugin for Craft CMS",
  "version": "1.0.0",
  "type": "craft-plugin",
  "autoload": {
    "psr-4": {
      "enupal\\translate\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "beeyev/yandex-translate": "dev-master"
  },
  "authors": [
    {
      "name": "Enupal",
      "homepage": "http://enupal.com/"
    }
  ],
  "support": {
    "email": "info@enupal.com",
    "issues": "https://github.com/enupal/translate/issues",
    "docs": "https://enupal.com/en/craft-plugins/enupal-translate/docs"
  },
  "extra": {
    "name": "Enupal Translate",
    "handle": "enupal-translate",
    "version": "1.0.0",
    "schemaVersion": "1.0.0",
    "hasSettings": true,
    "hasCpSection": true,
    "developer": "Enupal",
    "class": "enupal\\translate\\Translate",
    "components": {
      "app": "enupal\\translate\\services\\App"
    }
  }
}
