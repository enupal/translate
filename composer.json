{
  "name": "enupal/translate",
  "description": "Translate your website templates and plugins into multiple languages. Bulk translation with Google Translate or Yandex.",
  "version": "1.1.3",
  "type": "craft-plugin",
  "autoload": {
    "psr-4": {
      "enupal\\translate\\": "src/"
    }
  },
  "require": {
    "craftcms/cms": "^3.0.0-RC1",
    "beeyev/yandex-translate": "2.0.1",
    "stichoza/google-translate-php": "3.2.13",
    "viniciusgava/google-translate-api": "2.0.2"
  },
  "authors": [
    {
      "name": "Enupal",
      "homepage": "http://enupal.com/en"
    }
  ],
  "support": {
    "email": "info@enupal.com",
    "issues": "https://github.com/enupal/translate/issues",
    "docs": "https://enupal.com/en/craft-plugins/enupal-translate/docs"
  },
  "extra": {
    "name": "Enupal Translate",
    "handle": "enupal-translate",
    "schemaVersion": "1.0.0",
    "hasSettings": true,
    "developer": "Enupal",
    "class": "enupal\\translate\\Translate",
    "components": {
      "app": "enupal\\translate\\services\\App"
    }
  }
}
